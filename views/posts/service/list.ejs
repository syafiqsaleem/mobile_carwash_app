<%- include('../partials/header'); %>

<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Services</th>
      <th scope="col">Selection</th>
      <th scope="col">Price(SGD)</th>
      <th scope="col">Delete</th>
    </tr>
  </thead>
  <% let count = 0 %>
  <tbody>
    <% for (let key in service) { %>
    <tr>
      <th scope="row">
        <%=service[key] %><a href="/mobilecarwash/<%= key %>"
          ><button type="button"></button
        ></a>
      </th>
      <td>service</td>
      <td>price</td>
      <td>
        <form method="POST" action="/mobilecarwash/<%= key %>?_method=PATCH">
          <button type="submit" class="btn btn-danger" name="item" value="">
            DELETE
          </button>
        </form>
      </td>
    </tr>

    <%}%>
  </tbody>
</table>
<% if(count>5){ %>
<a href="/collection/new"
  ><button type="submit" class="btn btn-primary">IT'S TIME TO SHARE</button></a
>
<% }%> <%- include('../partials/footer'); %>
